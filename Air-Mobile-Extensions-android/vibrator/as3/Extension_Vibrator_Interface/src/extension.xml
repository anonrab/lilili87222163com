<extension xmlns="http://ns.adobe.com/air/extension/2.5">
	<id>de.patrickkulling.air.mobile.extensions.vibrator</id>
	<versionNumber>1</versionNumber>
	<platforms>
		<platform name="Android-ARM">
			<applicationDeployment>
				<nativeLibrary>vibrator.jar</nativeLibrary>
				<initializer>de.patrickkulling.air.mobile.extensions.vibrator.VibratorExtension</initializer>
				<finalizer>de.patrickkulling.air.mobile.extensions.vibrator.VibratorExtension</finalizer>
			</applicationDeployment>
		</platform>
		
		<platform name="iPhone-ARM">
			<applicationDeployment>
				<nativeLibrary>vibrator.a</nativeLibrary>
				<initializer>extensionInitializer</initializer>
				<finalizer>extensionFinalizer</finalizer>
			</applicationDeployment>
		</platform>
	</platforms>
</extension>